%!
% BEGIN SHAPE
297 385 translate
% END SHAPE

% BEGIN POLYGON
gsave
newpath
0 0 moveto
0 40 360 {
/angle exch def
angle rotate 72 0  rlineto
360 angle sub rotate
} for stroke
grestore
% END POLYGON

showpage

%!
% BEGIN SHAPE
297 385 translate
% END SHAPE

% BEGIN CIRCLE
gsave
0 0 moveto
newpath
0 0 72 0 360 arc
closepath
stroke
grestore
% END CIRCLE

showpage

%!
% BEGIN SHAPE
297 385 translate
% END SHAPE

% BEGIN RECTANGLE
gsave
newpath
0 0 moveto
-36 -72 rmoveto
0 72 rlineto
144 0 rlineto
0 -72 rlineto
-144 0 rlineto
closepath
stroke
grestore
% BEGIN RECTANGLE

showpage

